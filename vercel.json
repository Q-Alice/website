{
  "version": 2,
  "functions": {
    "api/**": {
      "runtime": "python3.9",
      "maxDuration": 60,
      "memory": 1024,
      "includeFiles": [
        "api/index.py",
        "requirements.txt",
        "api/qasm_files/**"
      ],
      "excludeFiles": [
        ".next/**",
        "*.cache/**",
        "node_modules/**",
        "public/**",
        "app/**",
        "tests/**",
        "docs/**",
        "__pycache__/**",
        "*.md",
        "*.log"
      ]
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
