{
    "version": 2,
    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python",
        "config": {
          "maxDuration": 60,
          "includeFiles": [
            "api/**/*.py",
            "Pipfile",
            "Pipfile.lock",
            "requirements.txt"
          ]
        }
      },
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/index.py"
      }
    ]
  }
  